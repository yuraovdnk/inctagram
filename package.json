{
  "name": "inctagram",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/inctagram/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/inctagram/test/jest-e2e.json --runInBand --forceExit",
    "build:inctagram": "nest build inctagram",
    "start:inctagram": "node dist/apps/inctagram/src/main",
    "start:dev:inctagram": "nest start inctagram --watch",
    "build:files-microservice": "nest build files-microservice",
    "start:files-microservice": "node dist/apps/files-microservice/src/main",
    "start:dev:files-microservice": "nest start files-microservice --watch",
    "start:dev:billing-service": "nest start billing-service --watch",
    "migrate:prod": "prisma migrate deploy",
    "migrate:test": "dotenv -e .env.test -- npx prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "prisma:gen-all": "yarn run prisma:gen:billing && yarn run prisma:gen:inctagram",
    "prisma:migrate-all": "prisma:gen:billing",
    "prisma:gen:billing": "prisma generate --schema prisma/billing/schema.prisma",
    "prisma:migrate:billing": "prisma migrate dev --schema prisma/billing/schema.prisma",
    "prisma:gen:inctagram": "prisma generate --schema prisma/schema.prisma",
    "prisma:migrate:inctagram": "prisma migrate dev --schema prisma/schema.prisma"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.412.0",
    "@nestjs/cache-manager": "^2.0.1",
    "@nestjs/common": "^10.4.15",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.4.15",
    "@nestjs/cqrs": "10.2.8",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/microservices": "^10.2.5",
    "@nestjs/mongoose": "^10.0.1",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.4.15",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^11.0.3",
    "@nestjs/throttler": "^4.2.1",
    "@prisma/client": "^5.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^5.0.0",
    "@types/multer": "^1.4.7",
    "@types/passport-github": "^1.1.7",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-jwt": "^3.0.9",
    "@types/passport-local": "^1.0.35",
    "bcrypt": "^5.1.0",
    "cache-manager": "^5.2.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "date-fns": "^2.30.0",
    "dotenv-cli": "^7.3.0",
    "eslint": "8.22.0",
    "latest": "^0.2.0",
    "mongoose": "^7.5.3",
    "nest-winston": "^1.9.4",
    "nestjs-rmq": "^2.14.0",
    "node-device-detector": "^2.0.17",
    "nodemailer": "^6.9.4",
    "passport": "^0.6.0",
    "passport-github": "^1.1.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "piscina": "^4.1.0",
    "prisma": "^5.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.1",
    "sharp": "^0.32.6",
    "stripe": "^17.4.0",
    "uuid": "^9.0.0",
    "vercel": "^31.2.1",
    "winston": "^3.10.0",
    "winston-mongodb": "^5.1.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.10",
    "@nestjs/schematics": "^9.2.0",
    "@nestjs/testing": "^10.4.15",
    "@types/jest": "28.1.4",
    "@types/node": "^22.10.5",
    "@types/nodemailer": "^6.4.8",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "28.1.2",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/"
    ]
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
